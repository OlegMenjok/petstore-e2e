{
  "name": "petstore-e2e",
  "version": "1.0.0",
  "description": "E2E backend tests for petStore api",
  "type": "commonjs",
  "main": "index.js",
  "scripts": {
    "test": "DEBUG=true mocha --config mocharc.js --reporter mocha-multi-reporters --reporter-options configFile=reporter.config.json",
    "test-integration": "DEBUG=false mocha --config mocharc.js --reporter mocha-multi-reporters --reporter-options configFile=reporter.config.json; node send-telegram.js",
    "test-serial": "DEBUG=true mocha --config mocharc.js --jobs 1 --parallel false --reporter mocha-multi-reporters --reporter-options configFile=reporter.config.json",
    "eslint": "eslint . --ext .ts",
    "eslint-fix": "eslint --fix",
    "prettier": "npx prettier --write .",
    "prettier-check": "npx prettier --check .",
    "prepare": "husky install"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/OlegMenjok/petstore-e2e.git"
  },
  "author": "Oleh Menok",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/OlegMenjok/petstore-e2e/issues"
  },
  "homepage": "https://github.com/OlegMenjok/petstore-e2e#readme",
  "dependencies": {
    "@faker-js/faker": "^10.2.0",
    "@types/mocha": "^10.0.10",
    "@types/node": "^22.0.0",
    "axios": "^1.13.2",
    "chai": "^6.2.2",
    "chalk": "^4.1.2",
    "dotenv": "^17.2.3",
    "eslint": "^8.7.0",
    "eslint-config-prettier": "^8.3.0",
    "husky": "^9.1.7",
    "joi": "^18.0.2",
    "mocha": "^11.7.5",
    "mocha-multi-reporters": "^1.5.1",
    "moment": "^2.30.1",
    "node-telegram-bot-api": "^0.67.0",
    "prettier": "^3.7.4",
    "superagent": "^10.3.0",
    "superagent-logger": "^1.1.0",
    "ts-node": "^10.9.0",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.0.0",
    "uuid": "^13.0.0",
    "winston": "^3.19.0"
  },
  "devDependencies": {
    "@types/chai": "^5.2.3",
    "@typescript-eslint/eslint-plugin": "^8.52.0",
    "@typescript-eslint/parser": "^8.52.0",
    "eslint-plugin-prettier": "^5.5.4"
  }
}
